<p id="notice"><%= notice %></p>

<h1>Workspaces</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Admin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @workspaces.each do |workspace| %>
      <tr>
        <td><%= workspace.name %></td>
        <td><%= workspace.admin %></td>
        
        <td><a href="/edit/<%= workspace.id%>">Edit</a></td>
        <td><a href="/destroy/<%= workspace.id%>">Destroy</a></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Workspace', "workspaces/new" %>
