<h1>Already Group</h1>

<table>

  <tbody>
    <% @group.each do |group| %>
      <tr>
       <td><%= group.name %></td>
        
        <td><%= group.purpose %></td>
       
        <td><%= group.access_type %></td>
        <td><%= link_to 'Show', group_path(group)%></td>
        <td><%= link_to 'Edit', edit_group_path(group) %></td>
        <td><%= link_to 'Destroy', group_path(group), method: "delete", data: { confirm: 'Are you sure?' } %></td>
        
      </tr>
    <% end %>   
  </tbody>
</table>
<div>
<%= link_to 'Back', new_group_path %>
</div>

